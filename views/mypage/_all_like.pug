// Nav tabs
ul#myTab.nav.nav-tabs.row.col-lg-9.col-12.mx-auto(role='tablist')
  li.nav-item
    a#roomlike-tab.nav-link.active(data-toggle='tab', href='#roomlike', role='tab', aria-controls='roomlike', aria-selected='true') 방 좋아요!!
  li.nav-item
    a#recipelike-tab.nav-link(data-toggle='tab', href='#recipelike', role='tab', aria-controls='recipelike', aria-selected='false') 레시피 좋아요!!
|
// Tab panes
.tab-content.row.col-lg-9.col-12.mx-auto
  #roomlike.tab-pane.active(role='tabpanel', aria-labelledby='roomlike-tab') 
    include _room_like
  #recipelike.tab-pane(role='tabpanel', aria-labelledby='recipelike-tab')
    include _recipe_like
